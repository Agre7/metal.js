{namespace Templates.NestedTestComponent}

/**
 */
{template .content}
	{call .components data="all" /}
{/template}

/**
 * @param count
 * @param foo
 * @param id
 * @param? extra
 */
{template .components}
	<div class="componentsWrapper">
	{for $index in range($count)}
		{call Templates.CustomTestComponent.content data="all"}
			{param headerContent: $foo /}
			{param id: $id + 'MyChild' + $index /}
			{param extra: $extra /}
		{/call}
	{/for}
	</div>
{/template}
