{
  "name": "metal-jsx",
  "version": "1.0.0-rc.16",
  "description": "A JSX templates renderer to be used with Metal.js's Component class",
  "license": "BSD",
  "repository": "metal/metal-jsx",
  "engines": {
    "node": ">=0.12.0",
    "npm": ">=3.0.0"
  },
  "jsnext:main": "src/JSXComponent.js",
  "main": "lib/JSXComponent.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "compile": "babel -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "keywords": [
    "metal"
  ],
  "babel": {
    "plugins": [
      [
        "babel-plugin-transform-es2015-classes",
        {
          "loose": true
        }
      ]
    ],
    "presets": [
      "metal-jsx",
      "es2015"
    ]
  },
  "dependencies": {
    "metal-component": "^1.0.0-rc.20",
    "metal-dom": "^1.0.0-rc.7",
    "metal-incremental-dom": "^1.0.0-rc.15"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-plugin-transform-es2015-classes": "^6.9.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-metal-jsx": "^0.0.2"
  }
}
