{
  "name": "metal-jsx",
  "version": "1.0.0-rc.8",
  "description": "A JSX templates renderer to be used with Metal.js's Component class",
  "license": "BSD",
  "repository": "metal/metal-jsx",
  "engines": {
    "node": ">=0.12.0",
    "npm": ">=3.0.0"
  },
  "main": "src/JSXComponent.js",
  "files": [
    "src"
  ],
  "scripts": {
    "test": "gulp test"
  },
  "keywords": [
    "metal"
  ],
  "babel": {
    "plugins": [
      [
        "incremental-dom",
        {
          "components": true,
          "prefix": "IncrementalDOM",
          "runtime": "iDOMHelpers"
        }
      ]
    ]
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "metal-component": "^1.0.0-rc.5",
    "metal-incremental-dom": "^1.0.0-rc.8",
    "metal-karma-config": "^2.0.1"
  },
  "devDependencies": {
    "babel-plugin-incremental-dom": "mairatma/babel-plugin-incremental-dom#build",
    "gulp": "^3.8.11",
    "gulp-metal": "^1.0.0",
    "metal-karma-config": "^2.1.0"
  }
}
